{
  "manifest_version": 3,
  "name": "__MSG_extension_name__",
  "description": "__MSG_extension_description__",
  "version": "1.4.0",
  "homepage_url": "https://github.com/asahisuenaga/rainbow-cursor",
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": ["https://docs.google.com/document/*"],
      "js": ["script.js"]
    }
  ],
"host_permissions": [
  "https://docs.google.com/document/*"
],
  "permissions": [
    "tabs",
    "storage",
    "scripting"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
"background": {
  "service_worker": "background.js"
},
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon48.png"
  }
}
